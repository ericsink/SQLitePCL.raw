<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(tfm_framework);$(tfm_net);$(tfm_windows)</TargetFrameworks>
    <LangVersion>$(ProviderLangVersion)</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\sn\SQLitePCLRaw.provider.e_sqlcipher.snk</AssemblyOriginatorKeyFile>
    <PackageDescription>SQLitePCLRaw is a Portable Class Library (PCL) for low-level (raw) access to SQLite.  Packages named 'SQLitePCLRaw.provider.*' (like this one) are 'plugins' that allow SQLitePCLRaw.core to access the native SQLite library.  This provider does DllImport of 'e_sqlcipher', the unofficial SQLCipher builds provided with SQLitePCLRaw.</PackageDescription>
  </PropertyGroup>

<ItemGroup>
	<ProjectReference Include="..\SQLitePCLRaw.core\SQLitePCLRaw.core.csproj" />
</ItemGroup>

<ItemGroup Condition=" '$(TargetFramework)' == '$(tfm_framework)' ">
    <Compile Include="Generated\provider_e_sqlcipher_prenet5_win.cs" />
</ItemGroup>

<ItemGroup Condition=" '$(TargetFramework)' == '$(tfm_net)' ">
    <Compile Include="Generated\provider_e_sqlcipher_funcptrs_notwin.cs" />
</ItemGroup>

<ItemGroup Condition=" '$(TargetFramework)' == '$(tfm_windows)' ">
    <Compile Include="Generated\provider_e_sqlcipher_funcptrs_win.cs" />
</ItemGroup>

</Project>
