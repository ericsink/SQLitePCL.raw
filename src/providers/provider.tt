        <#= KIND=="cil"?"unsafe ":"" #>int ISQLite3Provider.sqlite3_exec(sqlite3 db, utf8z sql, delegate_exec func, object user_data, out IntPtr errMsg)
        {
            int rc;

			<#= get_cb_type("exec") #> cb;
			exec_hook_info hi;
            if (func != null)
            {
				cb = <#= get_cb_val_nofield("exec") #>;
                hi = new exec_hook_info(func, user_data);
            }
            else
            {
				cb = null;
                hi = null;
            }
			var h = new hook_handle(hi);
            unsafe
            {
                fixed (byte* p_sql = sql)
                {
                    rc = NativeMethods.sqlite3_exec(db, p_sql, <#= get_cb_arg("cb") #>, h, out errMsg);
                }
            }
			h.Dispose();

            return rc;
        }

